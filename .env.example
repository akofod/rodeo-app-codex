# Supabase (Get these from your Supabase Dashboard -> Project Settings -> API)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key-here
# Server-only (keep secret, do not expose to the browser)
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key-here

# Mapbox (Get this from Mapbox Dashboard)
NEXT_PUBLIC_MAPBOX_TOKEN=pk.your-public-key-here

# App Config
NEXT_PUBLIC_APP_URL=http://localhost:3000
